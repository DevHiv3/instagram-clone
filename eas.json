{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "developmentClient": false
    },
    "staging": {
      "channel": "staging",
      "distribution": "internal",
      "developmentClient": true
    }
  },
  "submit": {
    "production": {}
  }
}
